# Contract Configuration
CONTRACT_ADDRESS=0x9BED32d2b562043a426376b99d289fE821f5b04E

# API Keys
ARBISCAN_API_KEY=your_arbiscan_api_key_here
RPC_ENDPOINT=your_rpc_endpoint_url_here
GRAPH_API_KEY=your_graph_api_key_here

# ENS Cache Configuration
# If set to "Y", use cached ENS data from ens_resolution.json
# If set to "N", fetch ENS names from the ENS subgraph
USE_CACHED_ENS=N

# Telegram Bot Configuration (Optional)
# To enable Telegram notifications:
# 1. Create a bot via @BotFather on Telegram
# 2. Get your bot token
# 3. Add it below (uncomment the line)
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Authentication Configuration (Optional - for dashboard access control)
# Secret key for signing authentication cookies (generate with: python3 -c "import os; print(os.urandom(32).hex())")
AUTH_COOKIE_SECRET=your_random_secret_key_here_change_this

# SMTP Email Configuration (for OTP delivery)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-gmail-app-password
SMTP_FROM=your-email@gmail.com

# Dashboard URL (for email links)
DASHBOARD_URL=https://dashboards.thegraph.foundation/reo/

